# Charles Schwab API Configuration
# Get these from https://developer.schwab.com/
SCHWAB_APP_KEY=your_schwab_app_key_here
SCHWAB_APP_SECRET=your_schwab_app_secret_here
SCHWAB_REDIRECT_URI=https://127.0.0.1

# AWS Configuration
# Available regions: us-east-1, us-west-2, eu-west-1, ap-southeast-1, sa-east-1 (Brazil)
AWS_REGION=sa-east-1
ENVIRONMENT=production

# Application Configuration
PORT=8080
APP_ENV=production
LOG_LEVEL=INFO
SECRET_KEY=your_flask_secret_key_here

# Local Development Only (not used on EC2)
SCHWAB_TOKEN_FILE=cs_tokens.json
